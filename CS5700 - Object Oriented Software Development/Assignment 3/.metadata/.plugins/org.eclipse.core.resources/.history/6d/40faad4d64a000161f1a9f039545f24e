package com.usu.command;

import java.awt.Point;

import javax.swing.JPanel;

public class DeselectCommand extends Command {
	JPanel panel;
	
	@Override
	public void execute() {
		targetDrawing.deselectAll(panel);
	}
	
	public DeselectCommand(Object[] commandParameters) {
		if (commandParameters.length > 0)
            panel = (JPanel) commandParameters[0];
	}
}
