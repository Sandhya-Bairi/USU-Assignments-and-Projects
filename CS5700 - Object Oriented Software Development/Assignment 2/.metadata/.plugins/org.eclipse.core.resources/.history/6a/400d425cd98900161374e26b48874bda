package com.usu.stocks;

public abstract class StockSubject {
	/**
	 * 
	 * @param addNewObserver
	 */
	public abstract void registerStockOberserver(int addNewObserver);

	/**
	 * 
	 * @param deleteObserver
	 */
	public void removeStockObserver(int deleteObserver);

	public void notifyStockObserver();

}