package test;

import java.net.DatagramSocket;
import java.net.InetAddress;

import org.junit.Test;

import com.usu.simulatorCommunication.messages.SimulatorCommunicator;
import com.usu.simulatorCommunication.messages.StreamStocksMessage;
import com.usu.stocks.Portfolio;

public class CommunicatorTest {
	
	SimulatorCommunicator scTest = new SimulatorCommunicator(new Portfolio());
	
	@Test
	public void testMonitoring_StreamStockMessageIsEmpty() {
		try {
			scTest.startUDPPacket();
			StreamStocksMessage startMessage = new StreamStocksMessage();
			scTest.send(startMessage);
			
			//assertNull(scTest.receivePacket(100));
		} catch (Exception e) {
			e.printStackTrace();
		}
	}
}
