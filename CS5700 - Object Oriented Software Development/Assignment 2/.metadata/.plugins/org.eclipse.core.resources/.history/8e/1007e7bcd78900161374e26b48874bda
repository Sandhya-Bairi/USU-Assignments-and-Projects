package com.usu.stocks;

public abstract class StockSubject {
	/**
	 * 
	 * @param addNewObserver
	 */
	public void subscribe(int addNewObserver) {
		throw new UnsupportedOperationException();
	}

	/**
	 * 
	 * @param deleteObserver
	 */
	public void unsubscribe(int deleteObserver) {
		throw new UnsupportedOperationException();
	}

	public void notifyStock() {
		throw new UnsupportedOperationException();
	}

}